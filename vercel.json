{
  "version": 2,
  "buildCommand": "echo 'Build complete'",
  "outputDirectory": "./",
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],
  "functions": {
    "api/*.js": {
      "maxDuration": 30
    }
  },
  "github": {
    "silent": true
  }
}
